{
  "model": "crewai-discovery-specialist",
  "training_data": "datasets/train.jsonl",
  "validation_data": "datasets/validation.jsonl",
  "epochs": 4,
  "batch_size": 8,
  "learning_rate": 5e-06,
  "objectives": [
    "tool_sequence_mastery",
    "task_completion_consistency",
    "error_recovery",
    "response_efficiency"
  ],
  "target_metrics": {
    "tool_accuracy": 0.95,
    "task_completion": 0.9,
    "error_recovery": 0.85,
    "response_efficiency": 0.75
  }
}